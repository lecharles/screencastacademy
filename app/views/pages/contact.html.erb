<h3>Contact</h3>
<div class="row z-depth-3">
  <div class="card-content">
    <p>Questions? Suggestions? Feedback? Visit? Please do.</p>
    <br>
    <div id="map"></div>
  </div>
</div>

<style>
  #map {
    width: 100%;
    height: 400px;
  }
</style>

<!-- Get right-click menu "what's here" option included working -->
<script>
  function initMap() {
    var mapDiv = document.getElementById('map');
    var map = new google.maps.Map(mapDiv, {
      center: {lat: 48.836134, lng: 2.235292},
      zoom: 12
    });
    var marker = new google.maps.Marker({
      position: {lat: 48.836134, lng: 2.235292},
      title: "Odigo HQ are here!"
    });

    marker.setMap(map);
  }
</script>

<script src="https://maps.googleapis.com/maps/api/js?callback=initMap" async defer></script>
